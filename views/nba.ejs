<%- include partials/header.ejs %>
<%- include partials/main_nav.ejs %>
<!-- Main Content -->
<main class="container">
    <div class = "row">
        <div class = "col-md-offset-3 col-md-6">
            <h1><%= title%></h1>
            <br>
    <table class= "table table-bordered table-striped table-hover">
    <tr>
        <th class= "text-center">Name</th>
        <th class= "text-center">Points</th>
        <th class= "text-center">Date</th>
        <th class= "text-center">Team</th>
        <th class= "text-center">Against</th>
    </tr>
    <!--repreat the template-->

    <% for(let count=0; count < Booklist.length; count++) { %>
        <tr>
            <td class= "text-center"><%= Booklist[count].name %></td>
            <td class= "text-center"><%= Booklist[count].points %></td>
            <td class= "text-center"><%= Booklist[count].date %></td>
            <td class= "text-center"><%= Booklist[count].team %></td>
            <td class= "text-center"><%= nbalist[count].against %></td>
        </tr>

    <% } %>
    </table>
</div>
</div>
</main>
<%- include partials/bottom_nav.ejs %>
